            <p>{{ text_slave }}</p>

            <p class="font-bold">{{ text_ttl }}</p>
            <p>{{ text_equipped }}</p>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-sm-2 text-center">
            <img src="/catalog/view/theme/imuto/image/technology/our-technology4.png" class="img-responsive">
          </div>
          <div class="col-xs-12 col-sm-10">
            <p class="font-bold">{{ text_im_power_technology }}</p>
