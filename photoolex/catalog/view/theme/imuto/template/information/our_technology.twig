            <p>{{ text_the_bulb }}</p>
            
            <p class="font-bold">{{ text_program }}</p>
            <p>{{ text_the_timed }}</p>
          </div>
        </div>
      </div>

      <div class="information-technology-item">
        <h3 class="text-uppercase information-technology-item-title">{{ text_fast_charge_technology }}</h3>
        <div class="row">
          <div class="col-xs-12 col-sm-2 text-center">
            <img src="/catalog/view/theme/imuto/image/technology/our-technology3.png" class="img-responsive">
          </div>
          <div class="col-xs-12 col-sm-10">
            <p class="font-bold">{{ text_power_delivery_ii_technology }}</p>
            <p>{{ text_power_delivery_ii_technology_description }}</p>

            <p class="font-bold">{{ text_color_technology }}</p>
            <p>{{ text_color_temperature_control_technology }}</p>

            <p class="font-bold">{{ text_metal_button }}</p>
            <p>{{ text_led }}</p>

