            <p class="font-bold">{{ text_strobe_mode }}</p>
            <p>{{ text_stroboscopic }}</p>

            <p class="font-bold">{{ text_backlight }}</p>
            <p>{{ text_detailed }}</p>

            <p class="font-bold">{{ text_fine }}</p>
